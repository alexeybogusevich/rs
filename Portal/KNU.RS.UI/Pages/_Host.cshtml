@page "/"
@namespace KNU.RS.UI.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>KNU.RS.UI</title>
    <base href="~/" />
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link rel="icon" type="image/png" href="~/img/sign-in/icons/favicon.ico" />
    <link rel="stylesheet" href="~/css/signin/fonts/font-awesome-4.7.0/css/font-awesome.css">
    <link rel="stylesheet" href="~/css/signin/fonts/iconic/css/material-design-iconic-font.css">
    <link rel="stylesheet" href="~/css/signin/vendor/bootstrap/css/bootstrap.css">
    <link rel="stylesheet" href="~/css/signin/vendor/animate/animate.css">
    <link rel="stylesheet" href="~/css/signin/vendor/css-hamburgers/hamburgers.css">
    <link rel="stylesheet" href="~/css/signin/vendor/animsition/css/animsition.css">
    <link rel="stylesheet" href="~/css/signin/vendor/select2/select2.css">
    <link rel="stylesheet" href="~/css/signin/vendor/daterangepicker/daterangepicker.css">
    <link rel="stylesheet" href="~/css/signin/util.css">
    <link rel="stylesheet" href="~/css/signin/main.css">
    <link href="css/site.css" rel="stylesheet" />
    <link href="KNU.RS.UI.styles.css" rel="stylesheet" />
</head>
<body>
    <component type="typeof(App)" render-mode="ServerPrerendered" />

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>

    <script src="_framework/blazor.server.js"></script>
</body>
</html>
